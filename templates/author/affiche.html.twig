<table>
<tr>
    <td>ID</td>
    <td>Username</td>
    <td>Email</td>
    <td>Actions</td>
</tr>



{% for author in authors %}
        <tr>
            <td>{{ author.id }}</td>
            <td>{{ author.username }}</td>
            <td>{{ author.email }}</td>
            <td><a href="{{ path('edit', {'id': author.id}) }}">Edit</a></td>
            <td><a href="{{ path('remove', {'id': author.id}) }}">Remove</a></td>
        </tr>
    {% endfor %}
</table>